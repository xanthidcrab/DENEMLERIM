<Window x:Class="CsvVisualizer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CsvVisualizer" xmlns:helix="http://helix-toolkit.org/wpf"
        mc:Ignorable="d"
        WindowState="Maximized"
       
      
        Title="MainWindow" Height="450" Width="800">

    <Grid>
        <helix:HelixViewport3D Name="myViewport" 
                          
                             ShowCameraInfo="True">
            <helix:HelixViewport3D.Camera>
                <PerspectiveCamera x:Name="myCamera" Position="0 0 10" LookDirection="0 0 -1" UpDirection="0 1 0" FieldOfView="45"/>
            </helix:HelixViewport3D.Camera>

            <ModelVisual3D>
                <ModelVisual3D.Content>
                    <Model3DGroup x:Name="myModelGroup">

                    </Model3DGroup>
                </ModelVisual3D.Content>
            </ModelVisual3D>
            <helix:SunLight/>
        </helix:HelixViewport3D>
    </Grid>

   
   
</Window>
